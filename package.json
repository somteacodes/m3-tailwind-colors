{
  "name": "m3-tailwind-colors",
  "version": "0.3.3",
  "description": "Generate Material 3 colors for Tailwind CSS (v3 and v4) and NativeWind.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "m3-tailwind-colors": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./v4": {
      "types": "./dist/v4.d.ts",
      "import": "./dist/v4.mjs",
      "require": "./dist/v4.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm run test"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-v4",
    "material-design",
    "material-3",
    "colors",
    "nativewind",
    "react-native",
    "css-variables"
  ],
  "author": "Somtochukwu Anunobi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/somteacodes/m3tailwindcolorgenerator"
  }
}
